@model IEnumerable<MainProjectNew.Models.Competition>

@{
    ViewBag.Title = "forStudent";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Award.CompetitionID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompetitionID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Detail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Condition)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Award.CompetitionID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompetitionID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Detail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Condition)
        </td>
        <td>
            @Html.ActionLink("Submit", "studentSubmit", "Exams", new { id = item.CompetitionID }, null) |
        </td>
    </tr>
}

</table>
